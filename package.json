{
  "name": "redis-odm",
  "version": "0.0.1",
  "description": "redis object document mapping",
  "keywords": [],
  "main": "dist/redis-odm.js",
  "files": [
    "dist"
  ],
  "author": "Bahman Asheghi <Asheghi.sh@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/asheghi/redis-odm"
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {},
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverageFrom": [
      "src/*.{js,ts}"
    ]
  },
  "dependencies": {
    "ioredis": "^5.2.3",
    "ulid": "^2.3.0",
    "zod": "^3.19.0"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^18.7.16",
    "jest": "^29",
    "ts-jest": "^29.0.0",
    "typescript": "^4.8.3"
  }
}
